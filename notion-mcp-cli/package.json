{
  "name": "notion-mcp-cli",
  "version": "0.0.6",
  "type": "module",
  "description": "CLI for Notion MCP with OAuth support",
  "bin": {
    "notion-mcp-cli": "bin.js"
  },
  "main": "./dist/cli.js",
  "types": "./dist/cli.d.ts",
  "files": [
    "dist",
    "bin.js"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "pnpm clean && tsc",
    "dev": "tsx src/cli.ts",
    "watch": "tsc -w",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "notion",
    "mcp",
    "cli"
  ],
  "author": "Tommaso De Rossi, morse <beats.by.morse@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@goke/mcp": "workspace:*",
    "goke": "workspace:*",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/node": "^22.15.21",
    "tsx": "^4.20.3",
    "typescript": "^5.9.3"
  }
}
